<Window x:Class="WPFChartApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFChartApp"
        xmlns:d3="clr-namespace:InteractiveDataDisplay.WPF;assembly=InteractiveDataDisplay.WPF" 
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded_1">
    <Grid>
        <d3:Chart BottomTitle="Time" LeftTitle="PhotoValue" Background="Ivory" Grid.ColumnSpan="2" >
            <d3:LineGraph x:Name="linegraph" Description="Photo Registor Value" 
                          Stroke="YellowGreen" StrokeThickness="4"/>
        </d3:Chart>

    </Grid>
</Window>
