<Window x:Class="BikeShop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BikeShop"
        mc:Ignorable="d"
        Height="{StaticResource pknu_Height}"
        Width="{StaticResource pknu_Width}"
        Title="{StaticResource pknu_Title}">
    <Grid Loaded="Grid_Loaded">

        <Frame x:Name="MainFrame" Source="/BikeShop;component/Menu.xaml" NavigationUIVisibility="Visible" />
    </Grid>
</Window>
