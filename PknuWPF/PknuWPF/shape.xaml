<Window x:Class="PknuWPF.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PknuWPF"
        mc:Ignorable="d"
        Title="Window1" Height="720" Width="1280">
    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Width="300">
        <Ellipse Fill="Aqua" Width="300" Height="200"/>
        <Rectangle Fill="Red" Width="300" Height="150" />
        <Path Height="100" Width="300" Fill="Blue">
            <Path.Data>
                <RectangleGeometry Rect="100,0,100,100"/>
            </Path.Data>

        </Path>


    </StackPanel>
</Window>
